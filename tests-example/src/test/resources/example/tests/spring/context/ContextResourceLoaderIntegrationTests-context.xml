<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd
">

  <!--<util:properties id="applicationProperties" location="context:application.properties"/>-->
  <util:properties id="applicationProperties"/>

  <bean id="name" abstract="true"><property name="name" value="TestApplication"/></bean>
  <bean id="log-level" abstract="true"><property name="log-level" value="trace"/></bean>

</beans>
